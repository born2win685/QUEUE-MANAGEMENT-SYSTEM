#main.png is for the first window

window.geometry("1200x849")
filename = tk.PhotoImage(file = "main.png")
background_label = tk.Label(window, image=filename)
background_label.place(x=0, y=0, relwidth=1, relheight=1)

nww.geometry("600x399")
bg = tk.PhotoImage(file="bricks.png")
label1 = tk.Label(nww, image=bg)
label1.place(x=0, y=0)

nww.geometry("600x399")
bg = tk.PhotoImage(file="bricks.png")
label1 = tk.Label(nww, image=bg)
label1.place(x=0, y=0)
  
quit.geometry("600x286")
filename = tk.PhotoImage(file="thankyou.png")
background_label_1 = tk.Label(quit, image=filename)
background_label_1.place(x=0, y=0)

